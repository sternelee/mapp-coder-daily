<block wx:if="{{$taroCompReady}}">
    <view class="index">
        <view class="header" style="{{anonymousState__temp}}">
            <view class="gengduo" bindtap="anonymousFunc0">
                <icon-font compid="{{$compid__2}}"></icon-font>
            </view>
            <view class="caidan" bindtap="anonymousFunc1">
                <icon-font compid="{{$compid__3}}"></icon-font>
            </view><text class="title">程序猿日常</text>
        </view>
        <view class="inner" style="{{anonymousState__temp2}}">
            <view class="topics">
                <view wx:key="id" class="topic" wx:for="{{topics}}" wx:for-item="v" wx:for-index="_anonIdx2">
                    <image src="{{v.image}}" mode="aspectFit"></image><text>{{v.name}}</text>
                </view>
            </view>
            <view class="posts">
                <view wx:key="id" class="post" wx:for="{{loopArray3}}" wx:for-item="v" wx:for-index="_anonIdx4">
                    <view class="topic">
                        <image src="{{v.$original.publication.image}}" mode="aspectFit"></image><text>{{v.$original.publication.name}}</text><text class="date">{{v.$original.publishedAt}}</text>
                    </view>
                    <view class="content">
                        <image src="{{v.$original.image}}"></image><text class="name">{{v.$original.title}}</text>
                        <view class="tip"><text wx:key="$loopState__temp4" wx:for="{{v.$anonymousCallee__1}}" wx:for-item="tag" wx:for-index="index">{{tag.$original}}</text>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</block>